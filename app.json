{
  "expo": {
    "name": "Grocery List App",
    "slug": "grocery-list-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2980B9"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.testwebstore"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#2980b9"
      },
      "package": "com.anonymous.testwebstore"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
